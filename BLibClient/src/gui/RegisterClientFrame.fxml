<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RegisterClientFrameController">
   <children>
      <Label layoutX="20.0" layoutY="40.0" text="First Name" />
      <TextField fx:id="firstNameTxt" layoutX="130.0" layoutY="35.0" prefWidth="200.0" />
      
      <Label layoutX="20.0" layoutY="80.0" text="Last Name" />
      <TextField fx:id="lastNameTxt" layoutX="130.0" layoutY="75.0" prefWidth="200.0" />
      
      <Label layoutX="20.0" layoutY="120.0" text="Phone Number" />
      <TextField fx:id="phoneTxt" layoutX="130.0" layoutY="115.0" prefWidth="200.0" />
      
      <Label layoutX="20.0" layoutY="160.0" text="Username" />
      <TextField fx:id="usernameTxt" layoutX="130.0" layoutY="155.0" prefWidth="200.0" />
      
      <Label layoutX="20.0" layoutY="200.0" text="Password" />
      <PasswordField fx:id="passwordTxt" layoutX="130.0" layoutY="195.0" prefWidth="200.0" />
      
      <Label layoutX="20.0" layoutY="240.0" text="Email" />
      <TextField fx:id="emailTxt" layoutX="130.0" layoutY="235.0" prefWidth="200.0" />
      
      <Button fx:id="registerBtn" layoutX="130.0" layoutY="280.0" mnemonicParsing="false" onAction="#onRegisterBtnClick" text="Register" />
   </children>
</AnchorPane>
