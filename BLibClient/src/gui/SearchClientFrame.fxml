<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SearchClientFrameController">
   <children>
      <Label layoutX="20.0" layoutY="40.0" text="Client Card ID" />
      <TextField fx:id="clientCardIdTxt" layoutX="130.0" layoutY="35.0" prefWidth="200.0" />
      <Button fx:id="searchBtn" layoutX="130.0" layoutY="80.0" mnemonicParsing="false" onAction="#onSearchBtnClick" text="Search" />
      <TextArea fx:id="resultArea" layoutX="20.0" layoutY="130.0" prefHeight="140.0" prefWidth="360.0" wrapText="true" editable="false" />
   </children>
</AnchorPane>
